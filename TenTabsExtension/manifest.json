{
  "name": "TenTabs",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Open multiple search tabs automatically with TenTabs!",
  "homepage_url": "http://hack.rice.edu",
  "browser_action": {
  },
  "icons": {
    "16": "icons/rice.png",
    "48": "icons/rice.png",
    "128": "icons/rice.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "management",
    "tabs",
    "storage"
  ]
}