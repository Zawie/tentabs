{
  "name": "TenTabs",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Open multiple search tabs automatically with TenTabs!",
  "homepage_url": "https://github.com/Zawie/tentabs",
  "browser_action": {
  },

  "commands": {
    "shiftright": {
      "suggested_key": {
        "default": "Ctrl+Right",
        "mac": "Command+Right"
      },
      "description": "Shift to right tab"
    },
    "shiftleft": {
      "suggested_key": {
        "default": "Ctrl+Left",
        "mac": "Command+Left"
      },
      "description": "Shift to left tab"
    }
  },

  "icons": {
    "16": "icons/rice.png",
    "48": "icons/rice.png",
    "128": "icons/rice.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "content_scripts": [{
    "matches": ["*://*.google.com/search?q*"],
    "js": ["src/content.js"]
  }],
  "permissions": [
    "management",
    "tabs",
    "storage",
    "webNavigation"
  ]
}